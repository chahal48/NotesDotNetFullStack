    {
        public static void Sub()
        {
            Demo.Add();
            //static block contain only static
            Console.WriteLine("The Value is :");
        }
        static void Main(string[] args)
        {
            Sub();// call for same class 
           // Demo.Add();// call for another class
        }
    }
