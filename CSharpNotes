using System;


namespace ConsoleApp21
{
    // Single level inher... 
    // only one parent and one child 
    public class Calculator // parent
    {
        public void Add()
        {
            Console.WriteLine("Hello Add");
        }
       
    }
    class Xyz : Calculator //child
    {
        // Hello add
        // Welcome to mY Page
        public void Show()
        {
            // base 
            Console.WriteLine("Hello Welcome");
            base.Add();
        }
        static void Main(string[] args)
        {
            Xyz xyz = new Xyz();
            xyz.Show();
        }
    }
}
