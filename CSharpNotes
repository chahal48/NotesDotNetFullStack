using System.Collections;

namespace ConsoleApp11
{
    internal class Program
    {
        static void Main(string[] args)
        {
            // group of object
            ArrayList obj = new ArrayList();
            obj.Add(12);
            obj.Add(131);
            obj.Add(114);
                obj.Add(154);
            
            obj.Sort();

            foreach (var item in obj)
            {
                Console.WriteLine(item);
            }



       
        }
    }
}
