    internal class Program
    {

        public static void ArrayLists()
        {
        // Hashset <T>  Store and fetch // (stop)dublicate value avail

            var Roll=new HashSet<int>();
            Roll.Add(10);
            Roll.Add(10);
            Roll.Add(40);
           
            foreach (var item in Roll)
            {
                Console.Write(item+" ");
            }

        
        }
        static void Main(string[] args)
        {

            ArrayLists();
        }
    }
