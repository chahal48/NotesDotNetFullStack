using System;

namespace ConsoleApp17
{
    internal class Program
    {
        static void Main(string[] args)
        {

            int[] Roll;//declare
            Roll = new int[6]; // initial
            // print all index
            for (int i = 0; i < 6; i++)
            {
                Console.Write(i+"\t");
                //Console.Write(Roll + "\t");
                //Console.Write(Roll[i] + "\t");
            }
            // find array length
            Console.WriteLine("Array Length" + Roll.Length);
            for (int i = 0; i < Roll.Length; i++)
            {
                Console.Write(i + "\t");
            }
        }
    }
}
________

using System;

namespace ConsoleApp17
{
    internal class Program
    {
        static void Main(string[] args)
        {

            int[] Roll;//declare
            Roll = new int[6]; // initial

            Roll[0] = 10;
            Roll[1] = 20;
            Roll[2] = 30;
            Roll[3] = 40;
            Roll[4] = 50;
            Roll[5] = 60;


            for (int i = 0; i < Roll.Length; i++)
            {
                Console.Write(i + "\t");
            }
            Console.WriteLine();
            for (int i = 0; i < Roll.Length; i++)
            {
                Console.Write(Roll[i]+"\t");
            }


        }
    }
}
