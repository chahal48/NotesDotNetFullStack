namespace ConsoleApp11
{
    internal class Program
    {
        static void Main(string[] args)
        {
            //  SortedSet - store , fetch and dublicate also
            // List<T>// type of class

            var Roll = new  SortedSet<double>();
            Roll.Add(1);
            Roll.Add(2.1);
            Roll.Add(333);
            Roll.Add(14);
            Roll.Add(53);
            Roll.Add(333);
            Roll.Add(6);
