using System;
class Program
{
    static void Main(string[] args)
    {
        // decimal 16 byte int+decimal
        // suffix  m

        string RollNo = "Hello";
        string Fee = "Amit@gmail.com";
        Console.WriteLine($"Fee amt is{Fee}  and Roll is {RollNo}");
        Console.WriteLine(String.Format($"Fee {Fee} Roll {RollNo}"));
        Console.WriteLine(string.Format($"Fee {Fee} Roll {RollNo}"));


    }
}

