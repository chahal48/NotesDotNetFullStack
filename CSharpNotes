using System;
class Program
{
    static void Main(string[] args)
    {
        // float 4 byte int+decimal
        // suffix f
      
        float RollNo = 2.1f;
        float Fee = 2200;
        Console.WriteLine($"Fee amt is{Fee}  and Roll is {RollNo}");
        Console.WriteLine(String.Format($"Fee {Fee} Roll {RollNo}"));
        Console.WriteLine(string.Format($"Fee {Fee} Roll {RollNo}"));


    }
}

