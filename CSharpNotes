    // Single level inher... 
    // only one parent and one child 
    public class Calculator // parent
    {
        public void Add()
        {
            Console.WriteLine("Hello Add");
        }
        public Calculator()
        {
            Console.WriteLine("hello cal const");
        }
    }
    class Xyz:Calculator //child
    {
      

        static void Main(string[] args)
        {
            Xyz xyz = new Xyz();    
            xyz.Add();
        }
    }
}
