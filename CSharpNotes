using System;

namespace ConsoleApp17
{
    internal class Program
    {
        static void Main(string[] args)
        {

            // dynamic array

            int[,] Roll = new int[2, 2];
            Roll[0, 0] = 1;
            Roll[0, 1] = 2;
            Roll[1, 0] = 3;
            Roll[1, 1] = 4;
            for (int i = 0; i < 2; i++)//row
            {
                for (int a = 0; a < 2; a++)// col
                {
    Console.Write(Roll[i, a]);
                }
                Console.WriteLine();
            }

        }
    }
}
