using System;
class Program
{
    static void Main(string[] args)
    {
        
      
        int RollNo = 21;
        int Fee = 2200;
        Console.WriteLine($"Fee amt is{Fee}  and Roll is {RollNo}");

        Console.WriteLine(String.Format($"Fee {Fee} Roll {RollNo}"));
        Console.WriteLine(string.Format($"Fee {Fee} Roll {RollNo}"));


    }
}

