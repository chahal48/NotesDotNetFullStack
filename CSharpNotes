        {

             int a=(int)Week.Mon;
            Console.WriteLine(a);

            int b = (int)Week.Sun;
            Console.WriteLine(b);
            DateTime dt = new DateTime();
            Console.WriteLine(DateTime.Now.DayOfWeek);

                


